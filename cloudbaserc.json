{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "functionRoot": "packages/tcb-functions",
  "functions": [
    {
      "name": "server",
      "timeout": 10,
      "runtime": "Nodejs10.15",
      "installDependency": true,
      "handler": "index.main",
      "ignore": [
        ".env",
        "*.md",
        "node_modules",
        "node_modules/**/*",
        "src",
        "src/**/*",
        "package-lock.json",
        "scripts",
        "scripts/*"
      ]
    }
  ],
  "framework": {
    "name": "fans"
  }
}